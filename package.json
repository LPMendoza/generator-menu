{
  "name": "automatic-generator-menu",
  "version": "0.1.0",
  "description": "Automatic generator of menu",
  "author": "willjw3",
  "build": {
    "appId": "gestionmenu.eg",
    "productName": "Gestión de menú",
    "win": {
      "icon": "build/logo.png"
    }
  },
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@popperjs/core": "^2.5.3",
    "bootstrap": "^4.5.3",
    "cross-env": "^6.0.3",
    "electron-is-dev": "^1.1.0",
    "excel-report": "^0.3.4",
    "excel4node": "^1.7.2",
    "jquery": "^3.5.1",
    "node-excel-export": "^1.4.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.2.0",
    "react-toastify": "^6.0.9",
    "xlsx-style": "^0.8.13"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "test-react": "react-scripts test --env=jsdom",
    "eject-react": "react-scripts eject",
    "build-electron": "electron-builder",
    "build": "npm run build-react && npm run build-electron",
    "start": "concurrently \"cross-env BROWSER=none npm run start-react\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.1.0",
    "wait-on": "^3.3.0"
  }
}
